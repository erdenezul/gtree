<h1><%=h @tab.title %></h1>

<b>Artist:</b>
<%=h @tab.artist %>
<br />
<b>Tag:</b>
<%=h @tab.tag %>
<br />
<b>User:</b>
<%= link_to User.find(@tab.user_id).username, User.find(@tab.user_id) %>

<pre>
  <%= @tab.tab %>
</pre>

<% if current_user %>
  <% if current_user.id == @tab.id %>
    <%= link_to 'Edit', edit_tab_path(@tab) %> |
  <% end %>
<% end %>
<%= link_to 'Back', tabs_path %>
